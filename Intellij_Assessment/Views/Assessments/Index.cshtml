@model IEnumerable<Intellij_Assessment.Assessment>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fdName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fdDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fdDateCreated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fdDateModified)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fdName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fdDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fdDateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fdDateModified)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.fdId }) |
            @Html.ActionLink("Details", "Details", new { id=item.fdId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.fdId })
        </td>
    </tr>
}

</table>
